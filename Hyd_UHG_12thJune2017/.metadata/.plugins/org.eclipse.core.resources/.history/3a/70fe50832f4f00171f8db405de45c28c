package com.basicsOfCoreJava;

public class Car {
	     String model;
	     int price;
	     int wheels;

	public static void main(String[] args) {
		
		Car a=new Car();
		Car b=new Car();
		Car c=new Car();
		
		a.model="BMW";
		a.price=20000;
		a.wheels=4;
		
		System.out.println(a.model);
		System.out.println(a.price);
		System.out.println(a.wheels);

	}

}
